package examsolution;

public class occurancechar {

	
	public static void main(String args[])
	{
		String str="india";
		int[] count =new int[256];
		for( int i=0;i<str.length();i++)
		{
			count[str.charAt(i)]++;
			
		}
		for(int i=0;i<256;i++)
		{
			
			if(count[i]!=0)
			{
				
				System.out.println((char)i+" is repeated="+ count[i] + "times");
						
			}
		}
	}
}
